class
extends thesis.lang.Container;

Text title;

regex #+(String title) {
	Chapter chapter = #(title, methodName.size());
	
	return chapter;
}

#(String title, Integer level) {
	new Chapter(title, level) chapter;
	content.add(chapter);
	
	return chapter;
}

newline(String parText) {
	new Paragraph(parText) p;
	content.add(p);
	
	return p;
}