[class]
extends thesis.lang.BaseContainer;

Text title;

regex #+(String title) {
	Chapter #(title, methodName.size()) chapter;
	
	return chapter;
}

#(String title, Integer level) {
	new Chapter(title, level) chapter;
	content.add(chapter);
	
	return chapter;
}

newline(String parText) {
	new Paragraph(parText) p;
	content.add(p);
	
	return p;
}