namespace neio.lang;

class Content implements neio.lang.Texable;

protected java.util.List<Content> content;

Content() {
    content = new java.util.ArrayList<Content>();
}

String toTex() {
	String tex = "";
	for (int i = 0; i < content.size(); i = i + 1) {
		tex = tex + "\n" + content.get(i).toTex();
	}
	
	return tex;
}
