namespace neio.stdlib;

import neio.lang.Content;
import neio.lang.Text;

class ParNLHandler extends NLHandler;

ParNLHandler(Paragraph parent) {
    super(parent);
}

ContainerNLHandler newline() {
    // parent should always have a parent
    return (TextContainer)(parent().parent()).newline();
}

Paragraph text(Text text) {
    return ((Paragraph) parent()).appendLine(text);
}
