namespace neio.stdlib;

import neio.lang.Content;
import neio.lang.Text;

class NLHandler;

Content parent; 

NLHandler(Content parent) {
    this.parent = parent;
}

Content parent() {
    return parent;
}

Paragraph text(Text text) {
    new Paragraph(text) par;
    parent().addContent(par);
    return par;
}

NLHandler newline() {
    return this;
}