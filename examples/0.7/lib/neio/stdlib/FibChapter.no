namespace neio.stdlib;

import neio.lang.Content;

class FibChapter extends Chapter;

FibChapter(String title, Integer level) {
	super(title, level);
}

nested FibChapter #(String title, Integer level) {
    new FibChapter(title, level) chapter;
    content.add(chapter);
	
    return chapter;
}

FibEnumerate -(String text) {
	FibEnumerate enumerate = new FibEnumerate();
	FibItem item = new FibItem(text, enumerate, 1);
	enumerate.-(item);
    content.add(enumerate);

    return enumerate;
}
