namespace neio.stdlib;

import neio.lang.Text;
import neio.lang.Content;

class Paragraph extends Content;

Text text;

Paragraph(String parText) {
	text = new Text(parText);
}

Paragraph(Text text) {
    this.text = text;
}

Text text() {
    return text();
}

String toTex() {
	return "\\par " + text.toTex();
}