namespace neio.fib;

import neio.stdlib.*;
import neio.lang.Text;
import neio.lang.Content;

class FibItem extends EnumerateItem;

@AssistedInject
FibItem(Factory factory, @Assisted String text, @Assisted Integer level) {
	super(factory, text, level);
}

@AssistedInject
FibItem(Factory factory, @Assisted String text, @Assisted Enumerate fib, @Assisted Integer level) {
	super(factory, text, fib, level);
}

String toTex() {
	return super.toTex(level + "." + enumerate.getNumber());
}