namespace neio.lang;

import java.util.*;

class Content implements Texable;

protected List<Content> content;

Content() {
    content = new ArrayList<Content>();
}

String toTex() {
    StringBuilder tex = new StringBuilder();
    for (int i = 0; i < content.size(); i = i + 1) {
        tex.append("\n").append(content.get(i).toTex());
    }

    return tex.toString();
}
